;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; This is the distributed configuration file for selenium and the extension 
; CentralNotice.
;
; Do not edit this file. If you would like to make changes, copy this file to 
; the same directory, removing the .dst extension:
;
; cp selenium.ini.dst selenium.ini
;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; selenium
;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[selenium]
;
; Screenshots are enabled by default 
;
screenshots	= yes
;
; Capture screenshots after assertion is enabled by default 
;
screenshotAfterAssertion	= yes
;
; Load selenese
;
selenese	= yes

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; host
;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[host]
;hostname	= "localhost"
hostname	= "wikimedia-commit.localhost.wikimedia.org"
;browserUrl	= "http://localhost/"
browserUrl	= "http://wikimedia-commit.localhost.wikimedia.org/"

[credentials]
admin[username] = "admin"
admin[password] = "mediawiki"

user[username]	= "user"
user[password]	= "mediawiki"
